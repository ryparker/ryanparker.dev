{
  "name": "ryanparker.dev",
  "private": true,
  "description": "Ryan Parker's personal portfolio site",
  "version": "1.2.2",
  "author": "Ryan Parker",
  "license": "UNLICENSED",
  "repository": "https://github.com/ryparker/ryanparker.dev.git",
  "bugs": {
    "url": "https://github.com/ryparker/ryanparker.dev/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "test": "start-server-and-test dev http://localhost:8000 cy:run",
    "test:ci": "yarn lint",
    "cy:open": "cypress open",
    "cy:run": "nyc cypress run --headless --browser electron",
    "cy:ci": "cypress run --headless --record --key cb6d03ef-840f-4014-bf24-c7d8a3fdc9fd --parallel",
    "cy:reset": "cypress run --env updateSnapshots=true",
    "fix": "xo . --fix",
    "lint": "xo .",
    "clean": "gatsby clean && cypress cache clear"
  },
  "xo": {
    "extends": [
      "xo-react",
      "plugin:cypress/recommended"
    ],
    "globals": [
      "window"
    ],
    "rules": {
      "camelcase": 0,
      "import/no-unassigned-import": 0,
      "import/extensions": 0,
      "no-useless-escape": 0,
      "react/boolean-prop-naming": 1,
      "no-alert": 1,
      "cypress/no-unnecessary-waiting": 1
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*": [
      "xo . --fix"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "emailjs-com": "^2.6.4",
    "formik": "^2.2.6",
    "gatsby": "^2.32.12",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-canonical-urls": "^2.10.0",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-gtag": "^2.8.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-nprogress": "^2.10.0",
    "gatsby-plugin-offline": "^3.10.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-scroll-reveal": "^0.0.7",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-source-graphql": "^2.14.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-particles-js": "^3.4.1",
    "react-scroll-parallax": "^2.3.5",
    "react-typed": "^1.2.0",
    "styled-components": "^5.2.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/node": "^14.14.28",
    "commitizen": "^4.2.3",
    "cypress": "^6.5.0",
    "cypress-image-snapshot": "^4.0.1",
    "cz-conventional-changelog": "3.3.00",
    "dotenv": "^8.2.0",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-config-xo-typescript": "^0.37.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "npm-run-all": "4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "^17.3.9",
    "start-server-and-test": "^1.12.0",
    "xo": "^0.37.1"
  },
  "keywords": [
    "ryanparker.dev",
    "portfolio",
    "developer",
    "gatsby",
    "react",
    "graphql",
    "aws"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
