    "gatsby-plugin-s3": "^0.3.2",
    "gatsby-plugin-scroll-reveal": "^0.0.4",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-plugin-sitemap": "^2.2.19",
